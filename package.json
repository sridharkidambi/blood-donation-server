{
    "name": "node_rest_api",
    "version": "1.0.0",
    "description": "A nodejs REST api seed project",
    "main": "src/server.js",
    "scripts": {
        "postinstall": "npx tsc",
        "seed-db": "npx tsc && node ./dist/seed-db.js",
        "start": "NODE_ENV=production npx pm2 start pm2.yaml",
        "console": "NODE_ENV=development tsc && node --experimental-repl-await ./dist/console.js",
        "dev": "tsc-watch --onSuccess \"node ./dist/server.js\"",
        "test": "NODE_ENV=test jest --coverage --verbose --detectOpenHandles",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/cors": "^2.8.5",
        "@types/express": "^4.17.0",
        "@types/jsonwebtoken": "^8.3.3",
        "@types/node": "^12.7.2",
        "@types/swagger-ui-express": "^3.0.1",
        "amqplib": "^0.5.5",
        "axios": "^0.19.0",
        "bcrypt": "^3.0.7",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.10.0",
        "cors": "^2.8.5",
        "custom-env": "^1.0.2",
        "express": "^4.17.1",
        "express-validator": "^6.1.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.26.0",
        "pg": "^8.2.1",
        "pm2": "^4.4.0",
        "reflect-metadata": "^0.1.13",
        "swagger-ui-express": "^4.0.7",
        "typeorm": "^0.2.25"
    },
    "devDependencies": {
        "@types/faker": "^4.1.12",
        "@types/jest": "^24.0.17",
        "@types/supertest": "^2.0.8",
        "faker": "^4.1.0",
        "jest": "^26.0.1",
        "supertest": "^4.0.2",
        "ts-jest": "^26.1.0",
        "tsc-watch": "^2.4.0",
        "typescript": "^3.5.3"
    }
}
